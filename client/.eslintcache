\n\r\n        <Row gutter={[16, 16]}>\r\n          {Movies &&\r\n            Movies.map((movie, index) => (\r\n              <React.Fragment key={index}>\r\n                <MovieRow\r\n                  image={\r\n                    movie.poster_path && `${IMAGE_URL}w500${movie.poster_path}`\r\n                  }\r\n                  movieId={movie.id}\r\n                />\r\n              </React.Fragment>\r\n            ))}\r\n        </Row>\r\n\r\n        {/* </div> */}\r\n\r\n                  \r\n      </div>\r\n    </main>\r\n  );\r\n};\r\n\r\nexport default Home;\r\n","D:\\BOOTCAMP\\UW-VIRT-FSF-PT-03-2022-U-LOL\\23-Project-3\\02-Challenge\\Film-Finder\\client\\src\\pages\\SingleThought.js",[],"D:\\BOOTCAMP\\UW-VIRT-FSF-PT-03-2022-U-LOL\\23-Project-3\\02-Challenge\\Film-Finder\\client\\src\\pages\\Signup.js",[],"D:\\BOOTCAMP\\UW-VIRT-FSF-PT-03-2022-U-LOL\\23-Project-3\\02-Challenge\\Film-Finder\\client\\src\\pages\\Login.js",[],"D:\\BOOTCAMP\\UW-VIRT-FSF-PT-03-2022-U-LOL\\23-Project-3\\02-Challenge\\Film-Finder\\client\\src\\pages\\MoviePage.js",[],"D:\\BOOTCAMP\\UW-VIRT-FSF-PT-03-2022-U-LOL\\23-Project-3\\02-Challenge\\Film-Finder\\client\\src\\pages\\Profile.js",[],"D:\\BOOTCAMP\\UW-VIRT-FSF-PT-03-2022-U-LOL\\23-Project-3\\02-Challenge\\Film-Finder\\client\\src\\components\\Header\\index.js",[],"D:\\BOOTCAMP\\UW-VIRT-FSF-PT-03-2022-U-LOL\\23-Project-3\\02-Challenge\\Film-Finder\\client\\src\\components\\Footer\\index.js",[],"D:\\BOOTCAMP\\UW-VIRT-FSF-PT-03-2022-U-LOL\\23-Project-3\\02-Challenge\\Film-Finder\\client\\src\\components\\config.js",[],"D:\\BOOTCAMP\\UW-VIRT-FSF-PT-03-2022-U-LOL\\23-Project-3\\02-Challenge\\Film-Finder\\client\\src\\components\\MoviesRow\\MoviesRow.js",[],"D:\\BOOTCAMP\\UW-VIRT-FSF-PT-03-2022-U-LOL\\23-Project-3\\02-Challenge\\Film-Finder\\client\\src\\components\\ImageComponent\\ImageComp.js",[],"D:\\BOOTCAMP\\UW-VIRT-FSF-PT-03-2022-U-LOL\\23-Project-3\\02-Challenge\\Film-Finder\\client\\src\\utils\\queries.js",[],"D:\\BOOTCA